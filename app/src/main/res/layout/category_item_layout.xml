<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_category_root"
    android:layout_width="70dp"
    android:layout_height="90dp"
    android:layout_marginStart="10dp"
    android:layout_marginTop="10dp"
    android:background="@drawable/item_home_category"
    android:orientation="horizontal"
    android:padding="10dp"
    android:paddingTop="20dp"
    android:paddingEnd="10dp">

    <ImageView
        android:id="@+id/img_cate_item_layout"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:backgroundTint="#7A0404" />

    <TextView
        android:id="@+id/tv_cate_item_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/img_cate_item_layout"
        android:backgroundTint="#111"
        android:gravity="center"
        android:text="hello"
        android:textColor="#111" />

